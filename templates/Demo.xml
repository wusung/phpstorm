<templateSet group="Demo">
  <template name="s-model-post" value="/**&#10; * The table associated with the model.&#10; *&#10; * @var string&#10; */&#10;protected $table = 'pt_posts';&#10;&#10;protected $fillable = [&#10;    'title',&#10;    'body'&#10;];" toReformat="false" toShortenFQNames="true">
