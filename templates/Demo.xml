  <template name="s-show-blade" value="@extends('layouts.app')&#10;&#10;@section('content')&#10;    &lt;div class=&quot;panel panel-default&quot;&gt;&#10;        &lt;div class=&quot;panel-heading&quot;&gt;檢視&lt;/div&gt;&#10;        &lt;div class=&quot;panel-body&quot;&gt;&#10;            &lt;form class=&quot;form-horizontal&quot; role=&quot;form&quot; method=&quot;POST&quot; action=&quot;{{ url('/post') }}&quot;&gt;&#10;                {{ csrf_field() }}&#10;                &lt;div class=&quot;form-group&quot;&gt;&#10;                    &lt;label for=&quot;title&quot; class=&quot;col-sm-2 control-label&quot;&gt;標題&lt;/label&gt;&#10;                    &lt;div class=&quot;col-sm-10&quot;&gt;&#10;                        &lt;input type=&quot;text&quot; class=&quot;form-control&quot; id=&quot;title&quot; name=&quot;title&quot; placeholder=&quot;Title&quot; readonly&gt;&#10;                    &lt;/div&gt;&#10;                &lt;/div&gt;&#10;                &lt;div class=&quot;form-group&quot;&gt;&#10;                    &lt;label for=&quot;body&quot; class=&quot;col-sm-2 control-label&quot;&gt;內文&lt;/label&gt;&#10;                    &lt;div class=&quot;col-sm-10&quot;&gt;&#10;                        &lt;input type=&quot;text&quot; class=&quot;form-control&quot; id=&quot;body&quot; name=&quot;body&quot; placeholder=&quot;Body&quot; readonly&gt;&#10;                    &lt;/div&gt;&#10;                &lt;/div&gt;&#10;            &lt;/form&gt;&#10;        &lt;/div&gt;&#10;    &lt;/div&gt;&#10;@endsection&#10;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
